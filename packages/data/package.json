{
  "name": "@sherpa/data",
  "version": "0.4.0",
  "private": true,
  "type": "module",
	"main": "./dist/index.js",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "prebuild": "shx rm -rf \"dist\"",
    "build": "run-s build:db build:ts",
    "build:ts": "tsc",
    "build:db": "tsx \"./scripts/copy-db.ts\" src dist",
    "dev": "nodemon --watch src --ext ts,db --on-change-only --exec \"npm run build --ignore-scripts\"",
    "lint": "eslint \"src\" \"scripts\"",
    "test:paths": "vitest paths",
    "test:resources": "vitest resources",
    "update:resources": "tsx \"scripts/resources/update.ts\""
  },
  "devDependencies": {
    "@seald-io/nedb": "^4.0.4",
    "@sherpa/configs": "*",
    "@types/he": "^1.2.3",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.7.2",
    "dotenv": "^16.4.1",
    "file-type": "^19.0.0",
    "he": "^1.2.0",
    "html-format": "^1.1.6",
    "nodemon": "^3.0.3",
    "npm-run-all2": "^6.1.1",
    "open": "^10.1.0",
    "playwright": "^1.41.1",
    "shx": "^0.3.4",
    "tsx": "^4.7.0",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
