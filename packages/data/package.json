{
  "name": "@sherpa/data",
  "version": "0.4.0",
  "private": true,
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "cpy \"src/**/*.json\" dist",
    "lint": "eslint \"src\""
  },
  "devDependencies": {
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "cpy-cli": "^5.0.0",
    "eslint-config-sherpa": "*",
    "rimraf": "^5.0.5",
    "tsconfig-sherpa": "*"
  }
}
