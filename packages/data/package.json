{
  "name": "@sherpa/data",
  "version": "0.4.0",
  "private": true,
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:ts && npm run build:json",
    "build:ts": "tsc",
    "build:json": "cpy \"src/**/*.json\" dist",
    "dev": "npm run dev:ts & npm run dev:json",
		"dev:ts": "nodemon --watch src --ext ts --on-change-only --exec \"npm run build:ts\"",
		"dev:json": "nodemon --watch src --ext json --on-change-only --exec \"npm run build:json\"",
    "lint": "eslint \"src\""
  },
  "devDependencies": {
    "@sherpa/configs": "*",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "cpy-cli": "^5.0.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5"
  }
}
